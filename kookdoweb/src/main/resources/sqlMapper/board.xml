<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="kookdo.com.sqlMapper.board">
    
    <select id="selectBoardPaged" resultType="BoardVO" parameterType="HashMap">
        
        SELECT * FROM (
        	SELECT ROWNUM AS RN, A.* FROM (
		        ( SELECT SUBJECT
		              , NOTE
		              , REMARK 
		           FROM KDBOARDTEST ) A ) )
		 WHERE RN BETWEEN #{start} and #{start}
    </select>
    
    <select id="selectBoard" resultType="boardVO">
        SELECT SUBJECT
             , NOTE
             , REMARK 
          FROM KDBOARDTEST
    </select>
    
</mapper>
